package javaprogram;

public class ArithmeticOperations {

	public static void main(String[] args) {
	   int x =75;
	   int y = 25;
	   int result;
	   result = x+y;
	   System.out.println(result); //addition 
	   
	   int a =75;
	   int b = 25;
	   result = a-b;
	   System.out.println(result); //Subtraction 
	   
	   int result1 = 4+5-6;
	   System.out.println(result1); 
	   
	   
	   int m = 67;
	   int n = 7;
	   int modulo;
	   modulo = m % n;
	   System.out.println(modulo); //modulus operator
	}

}
