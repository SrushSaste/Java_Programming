package javaprogram;

public class Array {

	public static void main(String[] args) {
		/*int rainfall[] = {23,34,56,65,78,80};
		rainfall[0]=11;
		System.out.println(rainfall[0]);*/
		 int squares[]= new int [10];
		 squares[0]=1;
		 squares[1]=4;
		 squares[2]=9;
		 squares[3]=16;
		 int n,x;
		 for(x=4;x<10;x=x+1)
		 {
			 n=x+1;
			 squares[x]=n*n;
		 }
		 System.out.println(squares[9]);
	}

}
